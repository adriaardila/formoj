{
  "name": "code16/formoj",
  "description": "Customizable form renderer",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }, {
    "name": "Antoine Guingand",
    "email": "antoine@code16.fr"
  }],
  "require": {
    "php": ">=7.1.3",
    "code16/sharp": "4.1.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "1.0.*",
    "phpunit/phpunit": "~8.0",
    "doctrine/dbal": "~2.5",
    "orchestra/testbench": "~3.8"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Formoj\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Code16\\Formoj\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Formoj\\FormojServiceProvider"
      ]
    }
  }
}
