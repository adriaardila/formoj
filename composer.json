{
  "name": "code16/formoj",
  "description": "Customizable form renderer",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }, {
    "name": "Antoine Guingand",
    "email": "antoine@code16.fr"
  }],
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^7.0|^8.0",
    "nesbot/carbon": "^2.0",
    "maatwebsite/excel": "^3.1",
    "ext-zip": "*",
    "laravel/legacy-factories": "^1.1"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "1.3.*",
    "phpunit/phpunit": "^8.5",
    "doctrine/dbal": "~2.5",
    "orchestra/testbench": "5.*|6.*",
    "code16/sharp": "^6.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Formoj\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "Code16\\Formoj\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Formoj\\FormojServiceProvider"
      ]
    }
  }
}
